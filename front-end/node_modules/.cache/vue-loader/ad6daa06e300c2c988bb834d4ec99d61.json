\" style=\"text-align: center\">\r\n                      <template slot=\"description\">\r\n                        <ellipsis :length=\"70\">{{ item.description | ellipsis }}</ellipsis>\r\n                      </template>\r\n                    </a-card-meta>\r\n                    <a-button type=\"primary\" class=\"cardItemContent\" ghost=true>\r\n                      <router-link :to=\"{ name: 'TestDetail', params:{ id: item.id,title:item.title} }\">开始测试</router-link>\r\n                    </a-button>\r\n                  </a-card>\r\n                </a-list-item>\r\n              </a-list>\r\n\r\n              <a-pagination @change=\"onChange\" :current=\"current\" :total=\"totalPage\" style=\"text-align: center\"/>\r\n\r\n            </div>\r\n\r\n          </a-row>\r\n        </a-card>\r\n      </a-card>\r\n    </a-col>\r\n  </a-row>\r\n\r\n</template>\r\n\r\n<script>\r\n  import moment from 'moment'\r\n  import {TagSelect, StandardFormRow, Ellipsis, AvatarList} from '@/components'\r\n  import {getTestList} from '@/api/psytest'\r\n  import {PageView} from '@/layouts'\r\n\r\n  export default {\r\n    components: {\r\n      PageView\r\n    },\r\n    data() {\r\n      return {\r\n        data: [],\r\n        allData: [],\r\n        loading: false,\r\n        current: 1,\r\n        totalPage: '',\r\n      }\r\n    },\r\n    filters: {\r\n      fromNow(date) {\r\n        return moment(date).fromNow()\r\n      },\r\n\r\n      /*省略多余描述*/\r\n      ellipsis (value) {\r\n        if (!value) return ''\r\n        if (value.length > 40) {\r\n          return value.slice(0,40) + '······'\r\n        }\r\n        return value\r\n      }\r\n\r\n    },\r\n    created() {\r\n      this.getList()\r\n    },\r\n\r\n    methods: {\r\n      onChange(current) {\r\n        this.current = current;\r\n        console.log(this.allData.id)\r\n        getTestList(this.current).then(response => {\r\n          console.log('点击输出', response.data)\r\n          this.allData = response.data\r\n          this.loading = false\r\n\r\n          this.data = this.allData.content\r\n          this.totalPage = this.allData.totalPage\r\n        })\r\n      },\r\n      getList() {\r\n        getTestList({'pageNum':this.current}).then(response => {\r\n          console.log('初始化成功', response.data)\r\n          this.allData = response.data\r\n          this.loading = false\r\n          this.data = this.allData.content\r\n          this.totalPage=this.allData.totalPage\r\n          console.log(this.data)\r\n        })\r\n\r\n      },\r\n\r\n    }\r\n  }\r\n\r\n\r\n\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n\r\n\r\n  .image-item {\r\n    width: 100%;\r\n    height: 100%;\r\n    float: left;\r\n    display: flex;\r\n    overflow: hidden;\r\n    align-items: center;\r\n    justify-content: center;\r\n  }\r\n\r\n  .button {\r\n    margin-top: 1rem\r\n  }\r\n\r\n  .ant-pro-components-tag-select {\r\n    /deep/ .ant-pro-tag-select .ant-tag {\r\n      margin-right: 24px;\r\n      padding: 0 8px;\r\n      font-size: 14px;\r\n    }\r\n  }\r\n\r\n  .ant-pro-pages-list-projects-cardList {\r\n    margin-top: 10px;\r\n    margin-left: 10px;\r\n    margin-right: 10px;\r\n\r\n    /deep/ .ant-card-meta-title {\r\n      margin-bottom: 4px;\r\n    }\r\n\r\n    /deep/ .ant-card-meta-description {\r\n      height: 66px;\r\n      overflow: hidden;\r\n      line-height: 22px;\r\n    }\r\n\r\n    .cardItemContent {\r\n      display: flex;\r\n      height: 20px;\r\n      margin-top: 16px;\r\n      margin-bottom: -4px;\r\n      line-height: 20px;\r\n\r\n      > span {\r\n        flex: 1 1;\r\n        color: rgba(0, 0, 0, .45);\r\n        font-size: 12px;\r\n      }\r\n\r\n      /deep/ .ant-pro-avatar-list {\r\n        flex: 0 1 auto;\r\n      }\r\n    }\r\n\r\n  }\r\n</style>\r\n"]}]}